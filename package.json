{
  "name": "lgpd-modal",
  "version": "1.0.0",
  "description": "Modal para aceite de termos LGPD no seu site/app",
  "main": "/src/main.js",
  "scripts": {
    "start": "npx sass --watch src/scss/main.scss dist/css/main.min.css && tsc -w && parcel watch dist/main.js --out-file dist/main.min.js",
    "styles": "npx sass --style=compressed src/scss/main.scss dist/css/main.min.css",
    "build": "tslint -c tslint.json 'src/**/*.ts' && tsc",
    "bundle": "parcel build dist/main.js --out-file dist/main.min.js",
    "clean": "rm -rf dist/*",
    "prepublish": "npm run build && npm run bundle && npm run styles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kvnol/lgpd-modal.git"
  },
  "keywords": [
    "modal",
    "lgpd"
  ],
  "author": "Kevin Oliveira <me@kevinoliveira.com.br>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kvnol/lgpd-modal/issues"
  },
  "homepage": "https://github.com/kvnol/lgpd-modal#readme",
  "devDependencies": {
    "prettier": "^2.3.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "parcel-bundler": "^1.12.5",
    "sass": "^1.37.5"
  }
}
